<h1>{{ section.settings.title }}</h1>
<p>{{ section.settings.subtext_html }}</p>

{% schema %}
{
  "name": "Template 404",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "label": "Title",
      "default": "404"
    },
    {
      "type": "richtext",
      "id": "subtext_html",
      "label": "Subtext",
      "default": "<p>Sorry, the page you are looking for does not exist.</p>"
    }
  ]
}
{% endschema %}
